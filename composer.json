{
    "name": "tiamo/phpas2",
    "description": "PHPAS2 is a php-based implementation of the EDIINT AS2 standard",
    "type": "library",
    "version": "2.0",
    "authors": [
        {
            "name": "Vladyslav K",
            "email": "vk.tiamo@gmail.com"
        }
    ],
    "keywords": [
        "edi",
        "ediint",
        "as2",
        "x12",
        "server"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1 || ^7.2 || ^7.3 || ^7.4 || 8.0",
        "ext-openssl": "*",
        "ext-zlib": "*",
        "guzzlehttp/guzzle": "^6.5 || ^7.0",
        "phpseclib/phpseclib": "^3.0.8",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "AS2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AS2\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "lint": "./vendor/bin/php-cs-fixer fix -v",
        "test": "./vendor/bin/phpunit --no-coverage",
        "test-cover": "./vendor/bin/phpunit --coverage-text"
    }
}
